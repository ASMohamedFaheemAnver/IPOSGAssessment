{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@providers/*": [
        "./src/providers/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@ui/*": [
        "./src/ui/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@typography/*": [
        "./src/typography/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
    },
  }
}